{
	"name": "t2fs",
  	"version": "0.0.1",
  	"private": true,
	  "scripts": {
		"dev": "vite dev",
		"start": "vite dev",
		"dev:host": "vite dev --host",
		"env:pull": "npx dotenv-vault pull",
		"env:push": "npx dotenv-vault push",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"deploy": "vercel deploy --prod --yes",
		"deploy:preview": "vercel --prod --yes",
		"fmt": "prettier --write --plugin-search-dir=. ./src/**/*.{js,ts,html,css,svelte,json}",
		"fmt:check": "prettier --check --plugin-search-dir=. ./src/**/*.{js,ts,html,css,svelte,json}",
		"git:update": "git add -A && git commit -m \"Update\" && git push"
	},
  	"devDependencies": {
		"@sveltejs/adapter-vercel": "^1.0.2",
		"@sveltejs/kit": "^1.3.9",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"@total-typescript/ts-reset": "^0.3.7",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"@tailwindcss/typography": "^0.5.9",
		"prettier-plugin-tailwindcss": "^0.2.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.0.4"
  	},
  	"type": "module",
  	"dependencies": {
		"sveltils": "^0.2.0",
		"firebase": "^9.16.0",
		"zod": "^3.20.6"
  	}
}	